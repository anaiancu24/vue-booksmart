{
  "manifest_version": 2,
  "name": "Booksmart",
  "version": "0.2.0",
  "description": "Smart bookmarking. Add folders and bookmark your current tabs to any folder.",
  "short_name": "B",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Booksmart",
    "default_icon": "favicon.ico"
  }
}
